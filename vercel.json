{
    "version": 2,
    "builds": [
      {
        "src": "src/index.ts",
        "use": "@vercel/node",
        "config": { "includeFiles": ["dist/*", "swagger/swagger-output.json"] }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "src/index.ts"
      },
      {
        "src": "/docs",
        "dest": "src/index.ts"
      }
    ]
  }
  